{
  "expo": {
    "name": "PhotoGoMobile",
    "slug": "PhotoGoMobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "scheme": [
      "photogo"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.mobile.PhotoGo"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-web-browser"
    ],
    "extra": {
      "eas": {
        "projectId": "adafe393-7132-412f-9616-d7965ad54c01"
      }
    },
    "updates": {
      "enabled": true,
      "url": "https://u.expo.dev/adafe393-7132-412f-9616-d7965ad54c01",
      "runtimeVersion": "sdkVersion"
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    }
  }
}
