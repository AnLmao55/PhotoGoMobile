{
  "expo": {
    "name": "PhotoGoMobile",
    "slug": "PhotoGoMobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logocam.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/logocam.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "scheme": [
      "photogo"
    ],
    "ios": {
      "supportsTablet": true,
      "runtimeVersion": {
        "policy": "appVersion"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logocam.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.mobile.PhotoGo",
      "runtimeVersion": "1.0.0",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-web-browser",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Cho phép PhotoGo sử dụng vị trí của bạn."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "adafe393-7132-412f-9616-d7965ad54c01"
      }
    },
    "updates": {
      "enabled": true,
      "url": "https://u.expo.dev/adafe393-7132-412f-9616-d7965ad54c01",
      "runtimeVersion": "1.0.0"
    },
    "permissions": [
      "location"
    ],
    "jsEngine": "hermes"
  }
}
